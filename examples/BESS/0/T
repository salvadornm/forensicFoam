FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 302;

boundaryField
{
    "(northbc|southbc|eastbc|westbc|topbc|bottombc)"
    {
        type            zeroGradient;
    }

    inner
    {
        type            zeroGradient;
    }

    default
    {
        type            zeroGradient;
    }

    inlet
    {
        type            fixedValue;
        value           uniform 302;
    }

    "(return1|return2)"
    {        
        type            zeroGradient;
    }

   "(backracksouth1|backracksouth2|backracksouth3)"
    {
        type            fixedValue;
        value           uniform 308;
       // type            zeroGradient;
    }

}


