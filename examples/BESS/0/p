FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    "(northbc|southbc|eastbc|westbc|topbc|bottombc)"
    {
        type            calculated;
        value           $internalField;
    }

    inner
    {
        type            calculated;
        value           $internalField;
    }

    default
    {
        type            calculated;
        value           $internalField;
    }

    "(inlet|return1|return2)"
    {
        type            calculated;
        value           $internalField;        
    }
}


