FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      ph_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1;

boundaryField
{

    "(northbc|southbc|eastbc|westbc|topbc|bottombc)"
    {
        type            fixedFluxPressure;
        value           $internalField;
    }

    inner
    {
        type            fixedFluxPressure;
        value           $internalField;
    }

    default
    {
        type            fixedFluxPressure;
        value           $internalField;
    }

    "(inlet|return1|return2)"
    {
       // type            fixedFluxPressure;
       // value           $internalField;
        type         zeroGradient;
    }

    "(backracksouth1|backracksouth2|backracksouth3)"
    {
        type            fixedFluxPressure;
        value           $internalField;
       //type            zeroGradient;
    }
}


