FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      C2H6;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    "(northbc|southbc|eastbc|westbc|topbc|bottombc)"
    {
        type            zeroGradient;
    }

    inner
    {
        type            zeroGradient;
    }

    default
    {
        type            zeroGradient;
    }

    "(inlet|return1|return2)"
    {
        type            zeroGradient;
    }

   "(backrack1|backrack2|backrack3|backrack4|backrack5|backrack6)"
    {
        type            zeroGradient;
    }

     hotbattery
    {
      //  type            zeroGradient;
        type            fixedValue;
        value           uniform 0.0141;
    } 
}


