FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      C2H2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    "(northbc|southbc|eastbc|westbc|topbc|bottombc)"
    {
        type            zeroGradient;
    }

    inner
    {
        type            zeroGradient;
    }

    default
    {
        type            zeroGradient;
    }

    "(inlet|inlet2|return1a|return1b|return2a|return2b|return3a|return3b|return4a|return4b)"
    {
        type            zeroGradient;
    }

   "(backrack1|backrack2|backrack3|backrack4|backrack5|backrack6)"
    {
        type            zeroGradient;
    }

     hotbattery
    {
       type            zeroGradient;
     //   type            fixedValue;
     //   value           uniform 0.0022;
    } 

    "fan.*"
    {
        type            zeroGradient;
    }

}


