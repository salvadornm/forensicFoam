    {
        type            zeroGradient;
    }

    "(inlet|inlet2)"
    {
        type            fixedValue;
        value           uniform 291.15;
    }

    "(return1a|return1b|return2a|return2b|return3a|return3b|return4a|return4b)"
    {        
        type            zeroGradient;
