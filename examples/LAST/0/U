FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.0 0 0);

boundaryField
{
    "(northbc|southbc|eastbc|westbc|topbc|bottombc)"
    {
        type            noSlip;
    }

    inner
    {
        type            noSlip;
    }

    default
    {
        type            noSlip;
    }

    inlet
    {
        type            fixedValue;
        value           uniform (9.5 0.0 0.0);
    }

    inlet2
    {
        type            fixedValue;
        value           uniform (-9.5 0.0 0.0);
    }

   "(return1a|return1b|return3a|return3b)"
    {
        type            fixedValue;
        value           uniform (0.0 4.5 0.0);  // bot +
    }

   "(return2a|return2b|return4a|return4b)"
    {
        type            fixedValue; 
        value           uniform (0.0 -4.5 0.0); // top -
    }

    "(backrack1|backrack2|backrack3)"
    {
       type            fixedValue;
       value           uniform (0.0 0.05 0.0);  // bot +
       type            noSlip;
    }

    "(backrack4|backrack5|backrack6)"
    {
        type            fixedValue;
        value           uniform (0.0 -0.05 0.0);  // top -
        type            noSlip;
    }

    hotbattery
    {
       type            fixedValue;
       value           uniform (0.0 0.5 0.0);  
      //  type            noSlip;
    }
    
}


