FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.0 0 0);

boundaryField
{
    "(northbc|southbc|eastbc|westbc|topbc|bottombc)"
    {
        type            noSlip;
    }

    inner
    {
        type            noSlip;
    }

    default
    {
        type            noSlip;
    }

    inlet
    {
        type            fixedValue;
        value           uniform (7.75 0.0 0.0);
    }

    inlet2
    {
        type            fixedValue;
        value           uniform (-7.75 0.0 0.0);
    }

   "(return1a|return1b|return3a|return3b)"
    {
        type            fixedValue;
        value           uniform (0.0 6.6 0.0);  // bot +
    }

   "(return2a|return2b|return4a|return4b)"
    {
        type            fixedValue; 
        value           uniform (0.0 -6.6 0.0); // top -
    }

    "(backrack1|backrack2)"
    {
        type            flowRateInletVelocity;
        volumetricFlowRate    constant  0.52;
        value           uniform (0 0 0);    
    }

    "(backrack3|backrack4|backrack5|backrack6)"
    {
        type            flowRateInletVelocity;
        volumetricFlowRate    constant  1.04;
        value           uniform (0 0 0);    
    }

    // "backrack.*"
    // {
    //    type            noSlip;
    // }

    
    hotbattery
    {
      // type            fixedValue;
      // value           uniform (0.0 0.1 0.0);  
       type            noSlip;
    }

    "fan.*"
    {
     type            flowRateInletVelocity;
     volumetricFlowRate    constant -0.02; // m3/s, negative means out domain
     value           uniform (0 0 0);

//       type            noSlip;
    }
    
}


