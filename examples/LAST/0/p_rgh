FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1; //1

boundaryField
{
    "(northbc|southbc|eastbc|westbc|topbc|bottombc)"
    {
        type            fixedFluxPressure;
        value           $internalField;   
       // type         zeroGradient;  
    }

    inner
    {
       type            fixedFluxPressure;
        value           $internalField;
      //  type         zeroGradient;  
    }

    default
    {
        type            fixedFluxPressure;
        value           $internalField;
      //  type         zeroGradient;  

    }

    "(inlet|inlet2|return1a|return1b|return2a|return2b|return3a|return3b|return4a|return4b)"
    {
        type            fixedFluxPressure;
        value           $internalField;
       //  type         zeroGradient;
    }

    "(backrack1|backrack2|backrack3|backrack4|backrack5|backrack6)"
    {
        type            fixedFluxPressure;
        value           $internalField;
       // type            zeroGradient;
    }

    hotbattery
    {
        type            fixedFluxPressure;
        value           $internalField;
       // type            zeroGradient;
    }

    "fan.*"
    {
        //type            calculated;
        //value           $internalField; 
        type zeroGradient;
    }
    
}


