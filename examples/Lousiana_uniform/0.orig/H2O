FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      H2O;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.0;

boundaryField
{
    "(north|south|east|west)"
    {
        type            zeroGradient;
    }

    top
    {
        type            zeroGradient;
    }

    "(bottom|terrain)"
    {
        type            zeroGradient;
    }

    tank1
    {
       // type            fixedValue;
       // value           uniform 0.0993;
        type            zeroGradient;
    }



    default
    {
        type            zeroGradient;
    }
  
}


