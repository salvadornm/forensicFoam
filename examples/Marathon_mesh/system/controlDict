FoamFile
{

    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application               snappyHexMesh;

startFrom                 startTime;

startTime                 0;

stopAt                    endTime;

endTime                   50;

deltaT                    1;

writeControl              timeStep;

writeInterval             20;

purgeWrite                0;

writeFormat               binary;

writePrecision            7;

writeCompression          no;

timeFormat                general;

timePrecision             6;

runTimeModifiable         yes;

graphFormat               raw;


