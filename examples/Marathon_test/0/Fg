FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

Finit          (0.0 0.0 0.0); 

dimensions      [1 -2 -2 0 0 0 0];

internalField    uniform $Finit;

boundaryField
{
    
   "(north|south|east|west|top|bottom|terrain)"
    {
        type           zeroGradient;      
    }

   "(tank1|tank2|containment)"
    {
        type            zeroGradient;
    }


}
