FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    "(north|south|east|west)"
    {
        // type            calculated;
        // value           $internalField;
        type            zeroGradient;

    }

    "(top|bottom)"
    {
      //  type            calculated;
      //  value           $internalField;
        type            zeroGradient;
    }
}

